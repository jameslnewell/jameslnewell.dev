  "scripts": {
    "format": "prettier --write '**/*.{md,json,js,jsx,ts,tsx,d.ts}'",
    "lint:typescript": "tsc",
    "lint:eslint": "eslint --ext .ts,.tsx src",
    "lint:prettier": "prettier --check '**/*.{md,json,js,jsx,ts,tsx,d.ts}'",
    "lint": "yarn run lint:typescript && yarn run lint:eslint && yarn run lint:prettier",
    "start": "next ./src",
