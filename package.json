    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/styled-components": "^4.1.6",
    "polished": "^3.0.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3",
    "styled-components-grid": "^2.2.2",
    "styled-components-spacing": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@jameslnewell/editor-config": "^1.0.1",
    "@jameslnewell/eslint-config": "^0.3.0",
    "@jameslnewell/prettier-config": "^1.0.1",
    "@jameslnewell/tslint-config": "^0.1.0",
    "@jameslnewell/typescript-config": "^2.0.2",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "^5.15.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "next": "^8.0.3",
    "next-images": "^1.1.1",
    "prettier": "^1.16.4",
    "prettier-check": "^2.0.0",
    "satay": "^1.1.6",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "lint:tslint": "tslint --project tsconfig.json",
    "lint:prettier": "prettier-check '**/*.{md,json,js,jsx,ts,tsx,d.ts}'",
    "lint:typescript": "tsc",
    "lint": "yarn run lint:tslint && yarn run lint:prettier && yarn run lint:typescript",
    "dev": "next ./src",
    "build": "next build ./src && next export -o ./.build ./src"
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx,d.ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
