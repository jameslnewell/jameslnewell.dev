{
  "name": "resume",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "polished": "^1.0.4",
    "prop-types": "^15.5.8",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-helmet": "^5.0.3",
    "sass-breakpoints": "^2.1.0",
    "sass-spacing": "^2.0.0",
    "smoothscroll": "^0.3.0",
    "styled-components": "^2.0.0-15",
    "styled-components-grid": "^1.0.0-preview.6",
    "styled-components-spacing": "^2.0.4",
    "site-ui": "file:./src/node_modules/site-ui"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "husky": "^0.13.3",
    "react-addons-test-utils": "^15.4.0",
    "react-testutils-query": "^0.10.0",
    "react-testutils-render": "^0.1.2",
    "tradie": "^4.0.0-rc.1",
    "tradie-template-react-static-site": "^4.0.0-rc.2"
  },
  "scripts": {
    "clean": "tradie clean",
    "build": "env NODE_PATH=./node_modules:./node_modules/tradie-template-react-static-site/node_modules tradie build --optimize",
    "watch": "npm run build -- --watch",
    "start": "env NODE_PATH=./node_modules:./node_modules/tradie-template-react-static-site/node_modules tradie serve",
    "test": "env NODE_PATH=./node_modules:./node_modules/tradie-template-react-static-site/node_modules tradie test",
    "prepush": "npm run build && npm run test",
    "upload:assets": "aws s3 sync --profile deploy-prod --delete ./dist s3://jameslnewell.me --exclude robots.txt --exclude \"*.html\" --exclude .DS_Store --expires \"2020-01-01T00:00:00Z\"",
    "upload:html": "aws s3 cp --profile deploy-prod ./dist/index.html s3://jameslnewell.me",
    "upload:robots": "aws s3 cp --profile deploy-prod ./src/robots.txt s3://jameslnewell.me",
    "upload": "npm run clean && npm run build && npm run upload:assets && npm run upload:html && npm run upload:robots"
  }
}
